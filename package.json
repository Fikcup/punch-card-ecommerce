    "migrate:revert:dev": "ts-node -r dotenv/config ./node_modules/typeorm/cli.js migration:revert -d ./src/database/connection.ts",
    "migrate:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/database/connection.ts"
